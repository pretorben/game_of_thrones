<%= link_to "Main", houses_path %>
<p><%= link_to "Create New House", new_house_path %> | <%= link_to "Create New Character", new_house_character_path(@house) %></p>
<article>
  <h2><%= @house.title %></h2>
  <%= image_tag @house.img_url %>
  <%= link_to "Edit House", edit_house_path(@house) %>
  <%= link_to "Delete House", house_path(@house), method: :delete %>
  <h4>Characters</h4>
  <% @house.characters.each do |character| %>
    <div>
      <h3><%= character.name %></h3>
      <%= image_tag character.img_url %>
      <%= link_to "Edit Character", edit_house_character_path(@house, character) %>
      <%= link_to "Delete Character", house_character_path(@house, character), method: :delete %>
    </div>
  <% end %>
</article>
